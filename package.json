{
  "name": "ddd-typescript-skeleton",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
    "build": "tsoa spec-and-routes && tsc && tsc-alias -p tsconfig.json",
    "start": "node build/src/app.js",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test:feature": "npm run build && NODE_ENV=test cucumber-js -p ci",
    "configure-eventbridge_sqs": "ts-node -r  tsconfig-paths/register ./src/scripts/configure-eventbridge_sqs.ts",
    "consume_sqs": "ts-node -r tsconfig-paths/register ./src/scripts/consume-sqs.ts",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-eventbridge": "^3.583.0",
    "@aws-sdk/client-sqs": "^3.583.0",
    "@tsoa/runtime": "^6.2.1",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "diod": "^2.0.0",
    "errorhandler": "^1.5.1",
    "express": "^4.19.2",
    "express-promise-router": "^4.1.1",
    "glob": "^10.4.1",
    "helmet": "^7.1.0",
    "http-status": "^1.7.4",
    "joi": "^17.13.1",
    "supertest": "^7.0.0",
    "swagger-ui-express": "^5.0.0",
    "tsoa": "^6.2.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.590.0",
    "@aws-sdk/lib-dynamodb": "^3.590.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@cucumber/cucumber": "^10.8.0",
    "@eslint/js": "^9.3.0",
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.5",
    "@types/errorhandler": "^1.5.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.12",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "concurrently": "^8.2.2",
    "eslint": "^9.3.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.4",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.10.0"
  },
  "lint-staged": {
    "*.(ts)": "npm run lint:fix"
  }
}
